{
    "name": "datapos-connector-file-store-emulator",
    "license": "MIT",
    "private": true,
    "type": "module",
    "version": "0.2.212",
    "devDependencies": {
        "@datapos/datapos-development": "^0.3.193",
        "@datapos/datapos-shared": "^0.3.255",
        "@types/node": "^24.10.1",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "csv-parse": "^6.1.0",
        "eslint": "^9.39.1",
        "eslint-plugin-import": "^2.32.0",
        "jiti": "^2.6.1",
        "license-report": "^6.8.1",
        "license-report-check": "^0.1.2",
        "nanoid": "^5.1.6",
        "npm-check-updates": "^19.1.2",
        "prettier": "^3.7.3",
        "typescript": "^5.9.3",
        "vite": "^7.2.4",
        "vite-plugin-dts": "^4.5.4"
    },
    "scripts": {
        "audit": "npm audit",
        "build": "vite build",
        "buildConnectorConfig": "node -e \"import('@datapos/datapos-development').then(m => m.buildConnectorConfig())\"",
        "bump:version": "node -e \"import('@datapos/datapos-development').then(m => m.bumpVersion())\"",
        "check": "npm outdated; npm-check-updates -i",
        "document": "license-report --only=prod,peer > LICENSES.json && license-report-check --source ./LICENSES.json --allowed 'MIT' --allowed 'n/a' --allowed 'Apache-2.0' --output=table",
        "format": "prettier --write src/",
        "lint": "eslint .",
        "publishToNPM": "echo \"***** PUBLISH TO NPM SCRIPT NOT IMPLEMENTED. *****\"",
        "release": "npm run syncWithGitHub && npm run build && npm run uploadModuleToR2 && npm run uploadModuleConfig",
        "sync": "npm run bump:version && npm run _sync:withGitHub",
        "update:dataPosDeps": "npm run _update:developDep && npm run _update:sharedDep",
        "_sync:withGitHub": "node -e \"import('@datapos/datapos-development').then(m => m.syncWithGitHub())\"",
        "_update:developDep": "npm install --save-dev @datapos/datapos-development@latest",
        "_update:sharedDep": "npm install @datapos/datapos-shared@latest",
        "uploadModuleConfig": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleConfig())\"",
        "uploadModuleToR2": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleToR2('datapos-connector-file-store-emulator.es.js', 'datapos-engine-eu/connectors/datapos-connector-file-store-emulator.es.js'))\""
    }
}