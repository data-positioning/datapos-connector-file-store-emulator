{
    "name": "datapos-connector-file-store-emulator",
    "license": "ISC",
    "private": true,
    "type": "module",
    "version": "0.1.925",
    "peerDependencies": {
        "@datapos/datapos-share-core": "^0.1.640",
        "csv-parse": "^5.6.0",
        "nanoid": "^5.0.8"
    },
    "devDependencies": {
        "@datapos/datapos-dev-operations": "^0.1.596",
        "@eslint/js": "^9.15.0",
        "@types/node": "^22.9.3",
        "eslint": "^9.15.0",
        "globals": "^15.12.0",
        "prettier": "^3.3.3",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.15.0",
        "vite": "^5.4.11",
        "vite-plugin-dts": "^4.3.0"
    },
    "scripts": {
        "audit": "npm audit",
        "build": "npx vite build",
        "bumpVersion": "node -e \"require('@datapos/datapos-dev-operations/scriptHelpers').bumpVersion()\"",
        "check": "npx npm-check-updates && npm outdated",
        "document": "npx npm-license-crawler --onlyDirectDependencies --production --relativeLicensePath --json LICENSES.json",
        "format": "prettier --write *.ts",
        "lint": "npx eslint src/**/*.ts",
        "publishToNPM": "echo \"***** PUBLISH TO NPM SCRIPT NOT IMPLEMENTED. *****\"",
        "release": "npm run syncWithGitHub && npm run build && npm run uploadPluginToR2",
        "syncWithGitHub": "npm run bumpVersion && node -e \"require('@datapos/datapos-dev-operations/scriptHelpers').syncWithGitHub()\"",
        "test": "echo \"***** TEST SCRIPT NOT IMPLEMENTED. *****\"",
        "updateDependencies": "npm run updateOperations",
        "updateOperations": "npm install --save-dev @datapos/datapos-dev-operations@latest",
        "uploadPluginToPagesViaGithub": "node -e \"require('@datapos/datapos-dev-operations/scriptHelpers').uploadPlugin()\"",
        "uploadPluginToR2": "npx wrangler r2 object put plugins-eu/connectors/datapos-connector-file-store-emulator-es.js --file=dist/datapos-connector-file-store-emulator-es.js --jurisdiction=eu --remote"
    }
}