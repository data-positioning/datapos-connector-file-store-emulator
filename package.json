{
    "name": "datapos-connector-file-store-emulator",
    "license": "ISC",
    "private": true,
    "type": "module",
    "version": "0.2.112",
    "peerDependencies": {
        "@datapos/datapos-share-core": "^0.1.787",
        "csv-parse": "^5.6.0",
        "date-fns": "^4.1.0",
        "nanoid": "^5.1.5"
    },
    "devDependencies": {
        "@datapos/datapos-dev-operations": "^0.2.11",
        "@eslint/js": "^9.24.0",
        "@types/node": "^22.14.1",
        "eslint": "^9.24.0",
        "globals": "^16.0.0",
        "prettier": "^3.5.3",
        "terser": "^5.39.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.30.1",
        "vite": "^6.3.0",
        "vite-plugin-dts": "^4.5.3"
    },
    "scripts": {
        "build": "npx vite build",
        "buildConfig": "node -e \"import('@datapos/datapos-dev-operations/scriptHelpers.js').then(m => m.buildConfig())\"",
        "bumpVersion": "node -e \"import('@datapos/datapos-dev-operations/scriptHelpers.js').then(m => m.bumpVersion())\"",
        "check": "npx npm-check-updates && npm outdated",
        "format": "prettier --write *.ts",
        "lint": "npx eslint src/**/*.ts",
        "release": "npm run syncWithGitHub && npm run build && npm run uploadConnectorConfig && npm run uploadConnectorToR2",
        "syncWithGitHub": "npm run bumpVersion && npm run buildConfig && node -e \"import('@datapos/datapos-dev-operations/scriptHelpers.js').then(m => m.syncWithGitHub())\"",
        "updateDependencies": "npm run updateOperations && npm run updateShareCore",
        "updateOperations": "npm install --save-dev @datapos/datapos-dev-operations@latest",
        "updateShareCore": "npm install @datapos/datapos-share-core@latest",
        "uploadConnectorConfig": "node -e \"import('@datapos/datapos-dev-operations/scriptHelpers.js').then(m => m.uploadConnectorConfig())\"",
        "uploadConnectorToR2": "npx wrangler r2 object put connectors/datapos-connector-file-store-emulator-es.js --file=dist/datapos-connector-file-store-emulator-es.js --content-type application/javascript --jurisdiction=eu --remote"
    }
}